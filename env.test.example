# Test Environment Variables
NODE_ENV=test

# Test Database (use a separate test database)
DATABASE_URL=postgresql://test:test@localhost:5432/agent_server_test

# Test Management Client
MANAGEMENT_CLIENT_ID=test-admin-client
MANAGEMENT_CLIENT_SECRET=test-admin-secret-123

# Test JWT Secret
MASTRA_JWT_SECRET=test-jwt-secret-for-testing

# Test Token Service Keys (Ed25519)
TOKEN_SERVICE_PRIVATE_KEY={"kty":"OKP","crv":"Ed25519","d":"test-private-key-base64","x":"test-public-key-base64","kid":"test-key-id"}
TOKEN_SERVICE_PUBLIC_KEY={"kty":"OKP","crv":"Ed25519","x":"test-public-key-base64","kid":"test-key-id"}

# Disable external services for testing
KEYS_DIR=test-keys
SERVERS_DB_FILE=test-servers.json
